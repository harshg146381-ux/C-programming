#include <stdio.h>

int main() {
    int num1, num2, result;

    // Taking input from user
    printf("Enter first number: ");
    scanf("%d", &num1);

    printf("Enter second number: ");
    scanf("%d", &num2);

    // Subtraction
    result = num1 - num2;

    // Display result
    printf("Result of subtraction: %d\n", result);

    return 0;
}
