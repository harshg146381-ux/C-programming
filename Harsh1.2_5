#include <stdio.h>
int main() {
    float gross, net;
    printf("Enter gross sales: ");
    scanf("%f", &gross);

    if (gross > 20000)
        net = gross - (gross * 0.15);
    else if (gross > 10000)
        net = gross - (gross * 0.10);
    else
        net = gross - (gross * 0.05);

    printf("Net Sales = %.2f", net);

    return 0;
}
